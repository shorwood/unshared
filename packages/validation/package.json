{
  "name": "@unshared/validation",
  "type": "module",
  "version": "0.7.4",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "directory": "packages/validation",
    "type": "git",
    "url": "git@github.com:shorwood/unshared.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./assert": {
      "require": "./dist/assert.cjs",
      "types": "./dist/assert.d.ts",
      "import": "./dist/assert.js"
    },
    "./createAssertionError": {
      "require": "./dist/createAssertionError.cjs",
      "types": "./dist/createAssertionError.d.ts",
      "import": "./dist/createAssertionError.js"
    },
    "./createParser": {
      "require": "./dist/createParser.cjs",
      "types": "./dist/createParser.d.ts",
      "import": "./dist/createParser.js"
    },
    "./createRule": {
      "require": "./dist/createRule.cjs",
      "types": "./dist/createRule.d.ts",
      "import": "./dist/createRule.js"
    },
    "./createRuleChain": {
      "require": "./dist/createRuleChain.cjs",
      "types": "./dist/createRuleChain.d.ts",
      "import": "./dist/createRuleChain.js"
    },
    "./createRuleMap": {
      "require": "./dist/createRuleMap.cjs",
      "types": "./dist/createRuleMap.d.ts",
      "import": "./dist/createRuleMap.js"
    },
    "./createRuleSet": {
      "require": "./dist/createRuleSet.cjs",
      "types": "./dist/createRuleSet.d.ts",
      "import": "./dist/createRuleSet.js"
    },
    "./isRuleLike": {
      "require": "./dist/isRuleLike.cjs",
      "types": "./dist/isRuleLike.d.ts",
      "import": "./dist/isRuleLike.js"
    },
    "./toAssert": {
      "require": "./dist/toAssert.cjs",
      "types": "./dist/toAssert.d.ts",
      "import": "./dist/toAssert.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@types/json-schema": "7.0.15",
    "@unshared/functions": "workspace:*",
    "@unshared/string": "workspace:*",
    "@unshared/types": "workspace:*"
  }
}
