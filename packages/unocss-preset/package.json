  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:shorwood/unshared.git",
    "directory": "packages/unocss-preset"
  },
  "exports": {
    "./preset": {
