    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@unocss/core": "0.59.0-beta.1",
    "@unocss/preset-mini": "0.59.0-beta.1",
    "@unshared/collection": "workspace:*",
    "@unshared/string": "workspace:*",
    "@unshared/types": "workspace:*"
