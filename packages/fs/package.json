{
  "name": "@unshared/fs",
  "type": "module",
  "version": "0.0.15",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "directory": "packages/fs",
    "type": "git",
    "url": "git@github.com:shorwood/unshared.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./createTemporaryDirectory": {
      "require": "./dist/createTemporaryDirectory.cjs",
      "types": "./dist/createTemporaryDirectory.d.ts",
      "import": "./dist/createTemporaryDirectory.js"
    },
    "./createTemporaryFile": {
      "require": "./dist/createTemporaryFile.cjs",
      "types": "./dist/createTemporaryFile.d.ts",
      "import": "./dist/createTemporaryFile.js"
    },
    "./findAncestor": {
      "require": "./dist/findAncestor.cjs",
      "types": "./dist/findAncestor.d.ts",
      "import": "./dist/findAncestor.js"
    },
    "./findAncestors": {
      "require": "./dist/findAncestors.cjs",
      "types": "./dist/findAncestors.d.ts",
      "import": "./dist/findAncestors.js"
    },
    "./glob": {
      "require": "./dist/glob.cjs",
      "types": "./dist/glob.d.ts",
      "import": "./dist/glob.js"
    },
    "./loadObject": {
      "require": "./dist/loadObject.cjs",
      "types": "./dist/loadObject.d.ts",
      "import": "./dist/loadObject.js"
    },
    "./touch": {
      "require": "./dist/touch.cjs",
      "types": "./dist/touch.d.ts",
      "import": "./dist/touch.js"
    },
    "./updateFile": {
      "require": "./dist/updateFile.cjs",
      "types": "./dist/updateFile.d.ts",
      "import": "./dist/updateFile.js"
    },
    "./withTemporaryDirectories": {
      "require": "./dist/withTemporaryDirectories.cjs",
      "types": "./dist/withTemporaryDirectories.d.ts",
      "import": "./dist/withTemporaryDirectories.js"
    },
    "./withTemporaryFiles": {
      "require": "./dist/withTemporaryFiles.cjs",
      "types": "./dist/withTemporaryFiles.d.ts",
      "import": "./dist/withTemporaryFiles.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@unshared/collection": "workspace:*",
    "@unshared/functions": "workspace:*",
    "@unshared/reactivity": "workspace:*",
    "@unshared/string": "workspace:*",
    "@unshared/types": "workspace:*"
  },
  "devDependencies": {
    "memfs": "4.8.2"
  }
}
