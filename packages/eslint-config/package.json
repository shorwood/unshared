{
  "name": "@unshared/eslint-config",
  "type": "module",
  "version": "0.4.0",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "directory": "packages/eslint-config",
    "type": "git",
    "url": "git@github.com:shorwood/unshared.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./configs": {
      "require": "./dist/configs.cjs",
      "types": "./dist/configs.d.ts",
      "import": "./dist/configs.js"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "peerDependencies": {
    "eslint": ">=9",
    "typescript": ">=5"
  },
  "dependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@stylistic/eslint-plugin": "2.12.1",
    "@unshared/collection": "workspace:*",
    "@unshared/types": "workspace:*",
    "@vitest/eslint-plugin": "1.1.21",
    "eslint-merge-processors": "0.1.0",
    "eslint-plugin-antfu": "2.7.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-html": "8.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "eslint-plugin-jsonc": "2.18.2",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-perfectionist": "4.4.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-sonarjs": "3.0.1",
    "eslint-plugin-unicorn": "56.0.1",
    "eslint-plugin-vue": "9.32.0",
    "eslint-plugin-yml": "1.16.0",
    "eslint-processor-vue-blocks": "0.1.2",
    "jsonc-eslint-parser": "2.4.0",
    "typescript-eslint": "8.18.2",
    "vue-eslint-parser": "9.4.3",
    "yaml-eslint-parser": "1.2.3"
  },
  "devDependencies": {
    "@types/eslint__eslintrc": "2.1.2",
    "@types/eslint__js": "8.42.3",
    "eslint": "9.17.0",
    "typescript": "5.7.2"
  }
}
