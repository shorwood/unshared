{
  "name": "@unshared/eslint-config",
  "type": "module",
  "version": "0.5.0",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "directory": "packages/eslint-config",
    "type": "git",
    "url": "git@github.com:shorwood/unshared.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./configs": {
      "require": "./dist/configs.cjs",
      "types": "./dist/configs.d.ts",
      "import": "./dist/configs.js"
    },
    "./utils": {
      "require": "./dist/utils.cjs",
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "peerDependencies": {
    "eslint": ">=9",
    "typescript": ">=5"
  },
  "dependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.28.0",
    "@stylistic/eslint-plugin": "4.4.1",
    "@unshared/collection": "workspace:*",
    "@unshared/types": "workspace:*",
    "@vitest/eslint-plugin": "1.2.2",
    "eslint-merge-processors": "2.0.0",
    "eslint-plugin-antfu": "3.1.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.7.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-markdown": "5.1.0",
    "eslint-plugin-n": "17.19.0",
    "eslint-plugin-perfectionist": "4.14.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-vue": "10.2.0",
    "eslint-plugin-yml": "1.18.0",
    "eslint-processor-vue-blocks": "2.0.0",
    "jsonc-eslint-parser": "2.4.0",
    "typescript-eslint": "8.34.0",
    "vue-eslint-parser": "10.1.3",
    "yaml-eslint-parser": "1.3.0"
  },
  "devDependencies": {
    "@types/eslint__eslintrc": "3.3.0",
    "@types/eslint__js": "9.14.0",
    "eslint": "9.28.0",
    "typescript": "5.8.3"
  }
}
