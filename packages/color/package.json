{
  "name": "@unshared/color",
  "type": "module",
  "version": "0.7.4",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "directory": "packages/color",
    "type": "git",
    "url": "git@github.com:shorwood/unshared.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./colorHexToRgb": {
      "require": "./dist/colorHexToRgb.cjs",
      "types": "./dist/colorHexToRgb.d.ts",
      "import": "./dist/colorHexToRgb.js"
    },
    "./colorHslToRgb": {
      "require": "./dist/colorHslToRgb.cjs",
      "types": "./dist/colorHslToRgb.d.ts",
      "import": "./dist/colorHslToRgb.js"
    },
    "./colorIntegerToRgb": {
      "require": "./dist/colorIntegerToRgb.cjs",
      "types": "./dist/colorIntegerToRgb.d.ts",
      "import": "./dist/colorIntegerToRgb.js"
    },
    "./colorRgbToHex": {
      "require": "./dist/colorRgbToHex.cjs",
      "types": "./dist/colorRgbToHex.d.ts",
      "import": "./dist/colorRgbToHex.js"
    },
    "./colorRgbToHsl": {
      "require": "./dist/colorRgbToHsl.cjs",
      "types": "./dist/colorRgbToHsl.d.ts",
      "import": "./dist/colorRgbToHsl.js"
    },
    "./colorRgbToInteger": {
      "require": "./dist/colorRgbToInteger.cjs",
      "types": "./dist/colorRgbToInteger.d.ts",
      "import": "./dist/colorRgbToInteger.js"
    },
    "./createColorHsl": {
      "require": "./dist/createColorHsl.cjs",
      "types": "./dist/createColorHsl.d.ts",
      "import": "./dist/createColorHsl.js"
    },
    "./createColorPalette": {
      "require": "./dist/createColorPalette.cjs",
      "types": "./dist/createColorPalette.d.ts",
      "import": "./dist/createColorPalette.js"
    },
    "./createColorRgb": {
      "require": "./dist/createColorRgb.cjs",
      "types": "./dist/createColorRgb.d.ts",
      "import": "./dist/createColorRgb.js"
    },
    "./types": {
      "require": "./dist/types.cjs",
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@unshared/math": "workspace:*",
    "@unshared/types": "workspace:*"
  }
}
