{
  "name": "@unshared/color",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "bugs": "https://github.com/shorwood/unshared/issues",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:shorwood/unshared.git",
    "directory": "packages/color"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./colorHexToSrgb": {
      "require": "./dist/colorHexToSrgb.cjs",
      "types": "./dist/colorHexToSrgb.d.ts",
      "import": "./dist/colorHexToSrgb.js"
    },
    "./colorHslToSrgb": {
      "require": "./dist/colorHslToSrgb.cjs",
      "types": "./dist/colorHslToSrgb.d.ts",
      "import": "./dist/colorHslToSrgb.js"
    },
    "./colorIntegerToSrgb": {
      "require": "./dist/colorIntegerToSrgb.cjs",
      "types": "./dist/colorIntegerToSrgb.d.ts",
      "import": "./dist/colorIntegerToSrgb.js"
    },
    "./colorSrgbToHex": {
      "require": "./dist/colorSrgbToHex.cjs",
      "types": "./dist/colorSrgbToHex.d.ts",
      "import": "./dist/colorSrgbToHex.js"
    },
    "./colorSrgbToHsl": {
      "require": "./dist/colorSrgbToHsl.cjs",
      "types": "./dist/colorSrgbToHsl.d.ts",
      "import": "./dist/colorSrgbToHsl.js"
    },
    "./colorSrgbToInteger": {
      "require": "./dist/colorSrgbToInteger.cjs",
      "types": "./dist/colorSrgbToInteger.d.ts",
      "import": "./dist/colorSrgbToInteger.js"
    },
    "./createColorHsl": {
      "require": "./dist/createColorHsl.cjs",
      "types": "./dist/createColorHsl.d.ts",
      "import": "./dist/createColorHsl.js"
    },
    "./createColorPalette": {
      "require": "./dist/createColorPalette.cjs",
      "types": "./dist/createColorPalette.d.ts",
      "import": "./dist/createColorPalette.js"
    },
    "./createColorSrgb": {
      "require": "./dist/createColorSrgb.cjs",
      "types": "./dist/createColorSrgb.d.ts",
      "import": "./dist/createColorSrgb.js"
    },
    "./types": {
      "require": "./dist/types.cjs",
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "@unshared/math": "workspace:*"
  }
}
