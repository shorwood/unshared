{
  "name": "@hsjm/shared",
  "version": "1.0.0",
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "description": "Shared utilities for @hsjm packages.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsjm-io/hsjm.git",
    "directory": "/home/shorwood/workspaces/hsjm/packages/shared"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./collection": {
      "require": "./dist/collection/index.js",
      "import": "./dist/collection/index.mjs",
      "types": "./dist/collection/index.d.ts"
    },
    "./color": {
      "require": "./dist/color/index.js",
      "import": "./dist/color/index.mjs",
      "types": "./dist/color/index.d.ts"
    },
    "./crypto": {
      "require": "./dist/crypto/index.js",
      "import": "./dist/crypto/index.mjs",
      "types": "./dist/crypto/index.d.ts"
    },
    "./misc": {
      "require": "./dist/misc/index.js",
      "import": "./dist/misc/index.mjs",
      "types": "./dist/misc/index.d.ts"
    },
    "./number": {
      "require": "./dist/number/index.js",
      "import": "./dist/number/index.mjs",
      "types": "./dist/number/index.d.ts"
    },
    "./regex": {
      "require": "./dist/regex/index.js",
      "import": "./dist/regex/index.mjs",
      "types": "./dist/regex/index.d.ts"
    },
    "./string": {
      "require": "./dist/string/index.js",
      "import": "./dist/string/index.mjs",
      "types": "./dist/string/index.d.ts"
    },
    "./validation": {
      "require": "./dist/validation/index.js",
      "import": "./dist/validation/index.mjs",
      "types": "./dist/validation/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENCE"
  ],
  "scripts": {
    "dev": "pnpm build -- --watch",
    "build": "tsup index.ts */index.ts --onSuccess 'pnpm build:post && pnpm build:lint'",
    "build:post": "esno ../../scripts/buildPost.ts",
    "build:lint": "eslint package.json --fix --no-ignore"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "axios": "^0.26.1"
  }
}
