{
  "name": "unshared-dev",
  "version": "1.0.0",
  "author": "Stanley Horwood <stanley@hsjm.io>",
  "description": "Opinionated collection of common components, composables and utils for Vue projects.",
  "license": "MIT",
  "homepage": "https://github.com/shorwood/unshared#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shorwood/unshared.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js",
      "node": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "@types/node": ">=12.0.0",
    "types-pkg-json": ">=1.0.0",
    "types-tsconfig": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "types-pkg-json": {
      "optional": true
    },
    "types-tsconfig": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@types/node": "18.11.18",
    "@vitest/coverage-c8": "0.27.2",
    "@vitest/ui": "0.27.2",
    "axios": "1.2.3",
    "c8": "7.12.0",
    "esbuild": "0.17.3",
    "esbuild-plugin-polyfill-node": "0.1.4",
    "esbuild-plugin-progress": "1.0.1",
    "esbuild-plugins-node-modules-polyfill": "1.0.9",
    "eslint": "8.32.0",
    "fast-glob": "3.2.12",
    "happy-dom": "8.1.4",
    "memfs": "3.4.13",
    "tsup": "6.5.0",
    "types-pkg-json": "1.2.1",
    "types-tsconfig": "2.0.2",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vitest": "0.27.2"
  },
  "eslintConfig": {
    "extends": [
      "./src/eslint-config/basic",
      "./src/eslint-config/typescript"
    ]
  },
  "pnpm": {
    "overrides": {
      "bl@<0.9.5": ">=0.9.5",
      "bl@<1.2.3": ">=1.2.3",
      "semver@<4.3.2": ">=4.3.2"
    }
  }
}
